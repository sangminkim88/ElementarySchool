<UserControl
    x:Class="GAIA2020.Design.UDrillLog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:Wpf="clr-namespace:HmDraw.View.Wpf;assembly=HmDraw.View.Wpf"
    mc:Ignorable="d" 
    d:DesignHeight="450" d:DesignWidth="800"
    Unloaded="UserControl_Unloaded" Loaded="UserControl_Loaded">
    <Grid>
        <Wpf:ViewControl x:Name="canvas" LoadComplete="Canvas_LoadComplete" MouseDown="Canvas_MouseDown" MouseUp="Canvas_MouseUp" PreviewKeyDown="Canvas_PreviewKeyDown"/>
    </Grid>
</UserControl>
