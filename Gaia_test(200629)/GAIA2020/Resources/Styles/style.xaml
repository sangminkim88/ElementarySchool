            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding IsMouseOver, ElementName=xGrid}" Value="True"/>
                    <Condition Binding="{Binding Source, ElementName=img, Converter={StaticResource NullToBooleanConverter}}" Value="True"/>
